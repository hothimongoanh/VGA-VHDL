FUNCTION abs_divider_obg (denominator[7..0], numerator[14..0])
RETURNS ( quotient[14..0], remainder[7..0]);

--synthesis_resources = lut 155 
SUBDESIGN lpm_divide_bso
( 
	denom[7..0]	:	input;
	numer[14..0]	:	input;
	quotient[14..0]	:	output;
	remain[7..0]	:	output;
) 
VARIABLE 
	divider : abs_divider_obg;
	numer_tmp[14..0]	: WIRE;

BEGIN 
	divider.denominator[] = denom[];
	divider.numerator[] = numer_tmp[];
	numer_tmp[] = numer[];
	quotient[] = divider.quotient[];
	remain[] = divider.remainder[];
END;
